#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
       int t;
       cin >> t;
       while(t--){
         int n;
         cin >> n;
         string a,b;
         cin >> a;
         cin >>  b;
         sort(a.begin(),a.end());
         sort(b.begin(),b.end());
         if(a==b){
             cout << "yes" << endl;
         }
         else{
             cout << "no" << endl;
         }
       }
}
