#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
  	int t;
  	cin >> t;
  	while(t--){
  	    int n;
  	    cin >> n;
  	    vector<int> a(n);
  	    int c =0;
  	    for(int i=0;i<n;i++){
  	        cin >> a[i];
  	        if(a[i]%2==0){
  	            c++;
  	        }
	    }
	    if(c==0 || c==n){
	        for(int i=0;i<n;i++){
	            cout << a[i] << " ";
	        }
	        cout << endl;
	    }
	    else{
	        sort(a.begin(),a.end());
	        for(int i=0;i<n;i++){
	            cout << a[i] << " ";
	        }
	        cout << endl;
	    }
	    
	}
 
}
