#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	    long long a,b,c,d;
	    cin >> a >> b >> c >> d;
	    if ( max(a, b) <= 2 * (min(a, b) + 1)  && c >= a && d >= b  && max(c - a, d - b) <= 2 * (min(c- a, d - b) + 1) ) {
    cout << "yes" << endl;
} else {
    cout << "no" << endl;
}
 
	}
 
}
