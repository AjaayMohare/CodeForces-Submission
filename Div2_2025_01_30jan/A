#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	    int n;
	    cin >> n;
	    vector<int> v(n+1);
	    v[n]=n;
	    v[0]=0;
	    int op =0 ;
	    for(int i=n-1;i>=1;i--){
	        if(op&1){
	            v[i]=v[i+1]+i;
	            op++;
	            
	        }
	        else{
	            v[i]=v[i+1]-i;
	            op++;
	        }
	    }
	    for(int i=1;i<=n;i++) cout << v[i] << " ";
	    cout << endl;
	    
	}
 
}
