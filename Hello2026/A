#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	    int n;
	    cin >> n;
	    vector<int> v(n);
	    int o=0;
	    int z=0;
	    for(int i=0;i<n;i++){
	      cin >> v[i];
	      if(v[i]==1) o++;
	      else z++;
	    } 
	    if(v[0]==1 || v[n-1]==1) cout << "Alice" << endl;
	    else cout <<"Bob" << endl;
	    
	}
 
}
