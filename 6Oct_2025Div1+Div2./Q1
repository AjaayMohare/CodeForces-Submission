#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	    int n;
	    cin >> n;
	   
	    vector<long long> v(n);
	    for(int i=0;i<n;i++){
	        cin >> v[i];
	    }
	    long long x;
	    cin >> x;
	    sort(v.begin(),v.end());
	    if(v[0]<=x && x <=v[n-1]){
	        cout << "yes" << endl;
	    }
	    else{
	        cout << "no" << endl;
	    }
	    
	    
	}
 
}
