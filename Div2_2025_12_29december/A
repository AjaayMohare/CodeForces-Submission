#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	int t;
	
	cin >> t;
	while(t--){
	    int n;
	    cin >> n;
	    string s;
	    cin >> s;
	    int co=0;
	    int f1=0;
	    int f2=0;
	    for(int i=0;i<=n-4;i++){
	        if(s.substr(i,4)=="2025") {f1=1;
	        co++;}
	        if(s.substr(i,4)=="2026") f2=1;
	    }
	    if(f2==1) cout << 0 << endl;
	    else if(f1==1)cout << 1 << endl;
	    else cout << 0 << endl;
	    
	}
 
}
