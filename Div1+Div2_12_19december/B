#include <bits/stdc++.h>
 
using namespace std;
 
int main() 
{
    // cout << "Hello, World!";
    int t;
    cin >> t;
    while(t--){
     int n;
     cin >> n;
     vector<string> v(n);
     for(int i=0;i<n;i++){
         cin >> v[i];
     }
     string ans="";
     ans+=v[0];
     for(int i=1;i<v.size();i++){
         if(ans+v[i]<v[i]+ans){
             ans+=v[i];
         }
         else{
             ans=v[i]+ans;
         }
     }
     cout << ans << endl;
    }
    return 0;
}
