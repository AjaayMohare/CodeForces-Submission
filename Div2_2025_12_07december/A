#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	    int n;
	    cin >> n;
	    vector<long long> v(n);
	    map<int,int> m;
	    for(int i=0;i<n;i++){
	        cin >> v[i];
	        m[v[i]]++;
	    }
	    v.push_back(m.size());
	    if(v[n]==v[n-1]){
	        cout << v[n] << endl;
	    }
	   else { while(v[v.size()-1]!=v[v.size()-2]){
	        m[v[v.size()-1]]++;
	        v.push_back(m.size());
	    }
	    cout << v[v.size()-1] << endl;}
	   
	}
 
}
