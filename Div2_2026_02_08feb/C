#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	   int n,k;
	   cin >> n >> k;
 
	   vector<int> v(n,0);
 
	   for(int i=0;i<k;i++){
	       string a;
	       cin >> a;
	       for(int j=0;j<n;j++){
	           v[j] |= (1<<(a[j]-'a'));
	       }
	   }
 
	   int rt = n;
 
	   // from now for every index cehck is there any coomon character by using bitwise 
	   // if at ay mooment bitwise comes zero we can use that length of d 
	   // if we cant find any d then cout any string 
	   // if we found d then find min t
	   // min t means that ther exist every common char 
	   for(int i=1;i<=n;i++){
 
	       if(n%i!=0) continue;
 
	       bool ok = true;
 
	       for(int j=0;j<i;j++){
 
	           int in = v[j];
 
	           for(int k=j;k<n;k+=i){
	               in &= v[k];
	           }
 
	           if(in==0){
	               ok=false;
	               break;
	           }
	       }
 
	       if(ok){
	           rt=i;
	           break;
	       }
	   }
 
	   string ans(n,'a');
 
	   for(int i=0;i<rt;i++){
 
	       int in = v[i];
 
	       for(int j=i;j<n;j+=rt){
	           in &= v[j];
	       }
 
	       char ch='a';
 
	       for(int k=0;k<26;k++){
	           if(in&(1<<k)){
	               ch='a'+k;
	               break;
	           }
	       }
 
	       for(int j=i;j<n;j+=rt){
	           ans[j]=ch;
	       }
	   }
 
	   cout<<ans<<endl;
	}
}
