#include <bits/stdc++.h>
using namespace std;
 
int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--){
	    long long n,m,x,y;
	    cin >> n >> m >>x >> y;
	    vector<long long> a(n),b(m);
	    for(int i=0;i<n;i++){
	        cin >> a[i];
	    }
	    for(int i=0;i<m;i++){
	        cin >> b[i];
	    }	    
	    
	    cout << n+m <<endl;
	    
	}
 
}
